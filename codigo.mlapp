            app.xEditField_2Label.Visible = false;
            app.yEditField.Visible = false;
            app.hEditField.Visible = false;
            app.hEditFieldLabel.Visible = false;
            app.yEditFieldLabel.Visible = false;
            app.SegmentsEditField.Visible = false;
            app.SegmentsLabel.Visible = false;

        end
        
        function results = graphicate(app,min,max,h)
            %delete everthing from graph
            cla(app.uiAxesFunction);
            % Graph the function
            title(app.uiAxesFunction, h);
            handle = matlabFunction(evalin(symengine, h));
            % Calculing the values
            position = (max-min)/100;
            x = min:position:max;
            y = handle(x);
            plot(app.uiAxesFunction,x,y,'Linewidth',2);
        end          

        function graphicate2func(app,min,max,h,v)
            display(v);
            %delete everthing from graph
            cla(app.uiAxesFunction);
            % Graph the function
            title(app.uiAxesFunction, h);
            handle = matlabFunction(evalin(symengine, h));
            % Calculing the values
            position = (max-min)/100;
            x = min:position:max;
            y = handle(x);
            plot(app.uiAxesFunction,v(1,:),v(2,:), '--bo', 'Linewidth',1,'MarkerFaceColor','b');
            hold(app.uiAxesFunction, 'on');
            plot(app.uiAxesFunction,x,y,'-r','linewidth',2,'MarkerFaceColor','r');     
            hold(app.uiAxesFunction, 'off')
        end
        
    end

