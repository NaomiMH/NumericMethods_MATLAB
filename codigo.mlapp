            app.MaxEditField_2Label.Visible = false;
            app.MaxEditField_2.Value = 0;
            app.MinEditField_2Label.Visible = false;
            app.MinEditField_2.Visible = false;
            app.MinEditField_2.Value = 0;
            app.RangeofIntervalLabel.Visible = false;
